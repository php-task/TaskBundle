{
    "name": "php-task/task-bundle",
    "description": "Symfony integration for php-task library",
    "type": "bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes.wachter@massiveart.at"
        }
    ],
    "require": {
        "php": "~5.5 || ~7.0",
        "php-task/php-task": "dev-feature/retry-execution",
        "symfony/http-kernel": "^2.6 || ^3.0",
        "symfony/dependency-injection": "^2.6 || ^3.0",
        "symfony/expression-language": "^2.6 || ^3.0",
        "symfony/config": "^2.6 || ^3.0",
        "symfony/console": "^2.6 || ^3.0",
        "symfony/process": "^2.6 || ^3.0",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/framework-bundle": "^2.6",
        "symfony/finder": "^2.6",
        "doctrine/doctrine-bundle": "^1.5",
        "doctrine/data-fixtures": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Task\\TaskBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Task\\TaskBundle\\Tests\\": "tests"
        }
    }
}
